{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["google"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "comma-dangle": 0,
        "max-len": [1, 100, 4],
        "linebreak-style": 0,
        "indent": 0,
        "require-jsdoc": 0,
        "object-curly-spacing": ["error", "always"],
        "quote-props": "warn"
    }
}
